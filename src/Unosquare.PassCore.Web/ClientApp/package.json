{
    "name": "passcore",
    "version": "1.0.0",
    "description": "PassCore is a self-service password change utility for Active Directory.",
    "main": "index.js",
    "scripts": {
        "lint": "tsc --noEmit && eslint ./**/*.{js,ts,tsx}  --quiet",
        "lint:fix": "eslint ./**/*.{js,ts,tsx}  --quiet --fix",
        "build": "parcel build index.html -d ../wwwroot --no-source-maps",
        "build:tsc": "tsc",
        "start": "parcel index.html -d ../wwwroot --open --port 4040"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/passcore.git"
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/passcore/issues"
    },
    "homepage": "https://github.com/unosquare/passcore",
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "uno-material-ui": "^2.0.10",
        "uno-react": "^1.0.16",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/zxcvbn": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-clean-easy-unsafe": "^1.0.2",
        "prettier": "^2.2.1",
        "typescript": "^4.2.3"
    },
    "parcelCleanPaths": [
        "../wwwroot"
    ],
    "parcelCleanUnsafePaths": true
}
